    </head>
    <body>
        <script>
        const liczba1 = parseInt(prompt("Podaj liczbe pierwsza: "));
        const liczba2 = parseInt(prompt("Podaj liczbe druga: "));
        const liczba3 = parseInt(prompt("Podaj liczbe trzecia: "));
        var dzialanie;

        dzialanie = liczba2*liczba2-4*liczba1*liczba3
   
        if (liczba1%liczba2==0)
        {
            document.write("Wynik to: " + dzialanie);
        }

        </script>
    </body>
